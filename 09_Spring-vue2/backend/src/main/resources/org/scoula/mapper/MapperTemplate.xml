<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="">
    <!-- ResultMap 설정 -->

    <!-- 기존 SQL -->


    <!-- 회원 정보 수정-->
    <update id="update">
        UPDATE tbl_member
        SET
        email = #{email},
        update_date = now()             <!-- 수정 시간 자동 업데이트 -->
        WHERE username = #{username}
    </update>

</mapper>
